local function AddToggle(page, name, default, callback)
    local f = Instance.new("Frame", page)
    f.Size = UDim2.new(1,-10,0,70)
    f.Position = UDim2.new(0,5,0,#page:GetChildren()*80)
    f.BackgroundColor3 = Color3.fromRGB(35,35,65)
    Instance.new("UICorner", f).CornerRadius = UDim.new(0,18)

    local lbl = Instance.new("TextLabel", f)
    lbl.Text = "  "..name
    lbl.Size = UDim2.new(1,-120,1,0)
    lbl.BackgroundTransparency = 1
    lbl.TextColor3 = Color3.new(1,1,1)
    lbl.Font = Enum.Font.GothamBold
    lbl.TextSize = 24
    lbl.TextXAlignment = "Left"

    local sw = Instance.new("TextButton", f)
    sw.Size = UDim2.new(0,110,0,50)
    sw.Position = UDim2.new(1,-115,0.5,-25)
    sw.BackgroundColor3 = default and Color3.fromRGB(0,255,140) or Color3.fromRGB(255,70,70)
    sw.Text = default and "ON" or "OFF"
    sw.TextColor3 = Color3.new(0,0,0)
    sw.Font = Enum.Font.GothamBold
    Instance.new("UICorner", sw).CornerRadius = UDim.new(0,30)

    local state = default
    sw.MouseButton1Click:Connect(function()
        state = not state
        TweenService:Create(sw, TweenInfo.new(0.2), {BackgroundColor3 = state and Color3.fromRGB(0,255,140) or Color3.fromRGB(255,70,70)}):Play()
        sw.Text = state and "ON" or "OFF"
        callback(state)
    end)

    page.CanvasSize = page.CanvasSize + UDim2.new(0,0,0,80)
end

local function AddButton(page, name, callback)
    local b = Instance.new("TextButton", page)
    b.Size = UDim2.new(1,-10,0,70)
