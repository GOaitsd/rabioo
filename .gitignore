    b.Font = Enum.Font.GothamBold
    b.TextSize = 24
    b.TextXAlignment = "Left"
    Instance.new("UICorner", b).CornerRadius = UDim.new(0,18)
    b.MouseButton1Click:Connect(callback)
    page.CanvasSize = page.CanvasSize + UDim2.new(0,0,0,80)
end

